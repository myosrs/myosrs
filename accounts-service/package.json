{
  "name": "accounts-service",
  "version": "1.0.0",
  "main": "build/app.js",
  "scripts": {
    "build": "tsc",
    "db": "typeorm",
    "dev": "nodemon --watch src -e tsx,ts,jsx,js --exec npm run dev:start",
    "dev:start": "npm-run-all build start --silent",
    "format": "prettier ./{src,__{tests,mocks}__}/**/**/*.{js,jsx,ts,tsx} --write",
    "lint": "eslint src --ext=\".js,.jsx,.ts,.tsx\"",
    "start": "node ./build/app.js",
    "test": "echo \"Write tests! -> myosrs/accounts-service\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-cron": "^2.0.3",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.3.6",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "@types/node-cron": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "license": "AGPL-3.0"
}
